  style.
    #half1 {
    margin-top: 50px;
    }
    #half2 {
    margin-top: 50px;
    margin-bottom: 50px;
    margin-left: 200px;
    margin-right: 200px;
    border: solid 2px;
    border-radius: 30px;
    }
    #half3 {
    margin-bottom: 50px;
    }
    .profile {
    text-align: center;
    padding-top: 20px;
    padding-bottom: 20px;
    }
    .profile img {
    border: solid 2px gray;
    border-radius: 20%;
    margin: 20px;
    transition: all ease 0.2s;
    }
    .profile img:hover {
    transform: scale(1.2);
    border: solid 2px purple;
    }
    .major > h2 {
        font-weight: bold;
    }

                  label(for="priority-high") High Priority
                .col-6.col-12-medium
                  input#copy(type="checkbox" name="copy")
                  label(for="copy") Email me a copy of this message
                .col-6.col-12-medium
                  input#human(type="checkbox" name="human" checked="")
                  label(for="human") I am a human and not a robot
                .col-12
                  textarea#message(name="message" placeholder="Enter your message" rows="6")
                .col-12
                  ul.actions
                    li
                      input.primary(type="submit" value="Send Message")
                    li
                      input(type="reset" value="Reset")
          
